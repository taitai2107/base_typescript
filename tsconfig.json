{
  "compilerOptions": {
      "target": "ES6",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "outDir": "build",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "types": ["jest"],
      "moduleResolution": "Node16",
      "module": "Node16"
  },
  "exclude": ["node_modules/"],
  "include": ["src/**/*.ts","src/test/**/*.ts"]
}